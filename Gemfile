source "https://rubygems.org"

gem "jekyll", "~> 4.3.3"

# If you want to use SCSS/SASS for styling
gem "jekyll-sass-converter", "~> 2.1"

# LESS support for Jekyll
gem "jekyll-less"

# If you need pagination support
gem "jekyll-paginate", "~> 1.1"

group :jekyll_plugins do
  gem "jekyll-archives"
  gem "jekyll-sitemap"
  gem "jekyll-seo-tag"
end